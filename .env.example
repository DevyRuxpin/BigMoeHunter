# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/bigmoehunter
REDIS_URL=redis://localhost:6379

# API Keys (Optional - App works without them)
# OPENAI_API_KEY=your_openai_api_key_here  # Not needed - using free AI
MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
WEATHER_API_KEY=your_weather_api_key_here

# NH Fish & Game API (if available)
NH_FISH_GAME_API_KEY=your_nh_fish_game_key_here

# App Configuration
APP_NAME=BigMoeHunter
APP_VERSION=1.0.0
DEBUG=True
SECRET_KEY=your_secret_key_here

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Mobile App Configuration
API_BASE_URL=http://localhost:8000
ENVIRONMENT=development

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/bigmoehunter.log
